{
   "$schema": "https://json.schemastore.org/nest-cli",
   "collection": "@nestjs/schematics",
   "sourceRoot": "src",
   "compilerOptions": {
      "assets": [
         {
            "include": "/**/*.hbs",
            "outDir": "dist/src",
            "watchAssets": true
         },
         {
            "include": "modules/**/*.png",
            "outDir": "dist/src",
            "watchAssets": true
         }
      ]
   }
}
